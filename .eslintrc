{
  "extends": "eslint:recommended",
  "rules": {
    // Errors
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "no-native-reassign": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-caller": 2,
    "no-new": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "max-params": [
      2,
      10
    ],
    "max-depth": [
      2,
      5
    ],
    "max-statements": [
      2,
      40
    ],
    "complexity": [
      2,
      8
    ],
    "max-len": [2, 120],
    "semi": [
      2,
      "always"
    ],
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "valid-typeof": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-shadow": 2,
    "dot-notation": 2,
    "no-new-func": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": false,
    "$": false,
    "app": false
  }
}
